{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.linchpin.com"
    }
  ],
  "require": {
    "php" : ">=7.1",
    "linchpin/gravityforms": "2.4.22",
    "linchpin/wp-rocket": "3.8.3",
    "linchpin/mantle": "1.0.0",
    "wpackagist-plugin/akismet": "4.1.8",
    "wpackagist-plugin/courier-notices": "1.3.1",
    "wpackagist-plugin/duplicate-post": "4.0.2",
    "wpackagist-plugin/imagify": "1.9.14",
    "wpackagist-plugin/jetpack": "9.4",
    "wpackagist-plugin/query-monitor": "3.6.7",
    "wpackagist-plugin/redirection": "5.0.1",
    "wpackagist-plugin/rewrite-rules-inspector": "1.2.1",
    "wpackagist-plugin/shortcode-in-menus": "3.5.1",
    "wpackagist-plugin/stream": "3.6.0",
    "wpackagist-plugin/wp-mail-smtp" : "2.6.0",
    "wpackagist-plugin/wordpress-seo": "15.7",
    "wpackagist-theme/twentytwentyone": "1.1"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  }
}